{
  "name": "ChatFriend",
  "version": "0.1.0",
  "private": true,
  "homepage": ".",
  "main": "electron/main.js",
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "concurrently": "^5.3.0",
    "electron-is-dev": "^1.2.0",
    "firebase": "^7.18.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3",
    "redux": "^4.0.5",
    "serverless-mysql": "^1.5.4",
    "socket.io-client": "^2.3.0",
    "typeface-poppins": "0.0.72",
    "wait-on": "^5.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "ebuild": "npm run build && node_modules/.bin/build",
    "dev": "concurrently \"npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron": "electron .",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "package-win": "electron-packager . ChatFriend --overwrite --asar=true --platform=win32 --arch=ia32 --icon=build/favicon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"ChatFriend\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "^9.2.0",
    "electron-packager": "^15.0.0",
    "electron-winstaller": "^4.0.1"
  }
}
